[
    {
        "scope": [
            "entity.name.tag.structure.any", //HTMl / body
            "meta.tag.sgml"
        ],
        "settings": {
            "foreground": "{{mainDarker2}}"
        }
    },
    {
        "scope": [
            "meta.tag",
            "meta.tag.any",
            "meta.tag.block.any"
        ],
        "settings": {
            "foreground": "{{mainDarker1}}BF"
        }
    },
    {
        "scope": [
            "meta.tag.inline.any"
        ],
        "settings": {
            "foreground": "{{main}}BF"
        }
    },
    {
        "scope": [
            "entity.name.tag.other.html"
        ],
        "settings": {
            "foreground": "{{mainLight2}}BF"
        }
    },
    {
        "scope": [
            "meta.tag.metadata.script"
        ],
        "settings": {
            "foreground": "{{mainAlt}}"
        }
    },
    {
        "scope": ["entity.other.attribute-name"],
        "settings": {
            "foreground": "{{booleans}}BF"
            // "fontStyle": "italic"
        }
    },
    {
        "scope": ["meta.toc-list.id"], //#ids
        "settings": {
            "foreground": "{{strings}}"
        }
    },
    {
        "scope": [
            "string.quoted.double.html",
            "string.quoted.jsx"
        ],
        "settings": {
            "foreground": "{{interpolated}}"
        }
    },
    {
        "scope": [
            "punctuation.definition.string.begin.html",
            "punctuation.definition.string.end.html",
        ],
        "settings": {
            "foreground": "{{strings}}7F"
        }
    }
]
