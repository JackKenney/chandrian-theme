{
	"name": "chandrian",
	"type": "dark",
	"colors": {
		"editor.background": "#262626",
		"editor.lineHighlightBackground": "#313131",
		"editorWhitespace.foreground": "#353434",
		"editor.foreground": "#FFFFFF",
		"editorCursor.background": "#91FF00",
		"editor.selectionBackground": "#474554",
		"editorOverviewRuler.bracketMatchForeground": "#fff000",
		"activityBarBadge.background": "#1E1E1E",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"variable.other.jsdoc", //variable name in jsdoc
				"other.description.jsdoc", // variable description
				"punctuation.definition.comment",
				"storage.type.class.jsdoc"  //@param
			],
			"settings": {
				"foreground": "#757575"
			}
		},
		{
			"scope": [
				"entity.name.type.instance.jsdoc" //{string}
			],
			"settings": {
				"foreground": "#B1B1B1"
			}
		},
		{
			"scope": [
				"meta.brace",
				"punctuation.terminator",
				"punctuation.definition",
				"punctuation.separator",
				"meta.delimiter"
			],
			"settings": {
				"foreground": "#757575"
			}
		},
		{
			"scope": [
				"meta.function-call.without-arguments.js"
			],
			"settings": {
				"foreground": "#B1B1B1"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#ca4646"
			}
		},
		{
			"scope": [
				"string.quoted.js",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#A4C35B"
			}
		},
		{
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#7494CD"
			}
		},
		{
			"scope": [
				"constant.language.null",
				"constant.language.undefined"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#976FB1"
			}
		},
		{
			"scope": [
				"constant.language.boolean"
			],
			"settings": {
				"foreground": "#976FB1"
			}
		},
		{
			"scope": [
				"string.interpolated.js"
			],
			"settings": {
				"foreground": "#D98270"
			}
		}, 
		{
			"scope": [
				"constant.other.object.key.js"
			],
			"settings": {
				"foreground": "#D0CE59"
			}
		},
		{
			"scope": [
				"storage.type",
				"keyword.operator.assignment"
			],
			"settings": {
				"foreground": "#787586"
			}
		},
		{
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#F2ECFF"
			}
		},
		{
			"scope": [
				"entity.name.function",
				"variable.function"
			],
			"settings": {
				"foreground": "#FFCC66"
			}
		},
		{
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#CE9A29"
			}
		},
		{
			"scope": [
				"keyword.control.conditional",
				"keyword.control.switch"
			],
			"settings": {
				// "fontStyle": "italic"
			}
		},
		{
			"scope": [
				"keyword.control.flow",
				"keyword.control.trycatch",
				"variable.function meta.function-call"
			],
			"settings": {
				"foreground": "#EF685D"
				, "fontStyle": "italic"

				// ,"fontStyle": "bold"
			}
		}, 
		{
			"scope": [
				"markup.bold.markdown"
			],
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": [
				"markup.italic.markdown"
			],
			"settings": {
				"foreground": "#FFF",
				"fontStyle": "italic"
			}
		},
		
		{
			"scope": [
				"markup.underline.link.markdown"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": [
				"markup.quote.markdown"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"text.html.markdown",
				"text.html.basic"
			],
			"settings": {
				"foreground": "#FFFFFFBF"
			}
		},
		{
			"scope": [
				"heading.1.markdown",
				"markup.heading.setext.1.markdown"
			],
			"settings": {
				"foreground": "#19B5FE",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"heading.2.markdown",
				"markup.heading.setext.2.markdown"
			],
			"settings": {
				"foreground": "#19B5FE"
			}
		},
		{
			"scope": [
				"heading.3.markdown"
			],
			"settings": {
				"foreground": "#6CCFFE"
			}
		},
		{
			"scope": [
				"heading.4.markdown"
			],
			"settings": {
				"foreground": "#C0EAFE"
			}
		},
		{
			"scope": [
				"heading.5.markdown",
				"heading.6.markdown"
			],
			"settings": {
				"foreground": "#EAF8FE"
			}
		},
		{
			"scope": [
				"beginning.punctuation.definition.list.markdown"
			],
			"settings": {
				"foreground": "#19B5FEBF"
			}
		},
		{
			"scope": [
				"punctuation.definition.string.markdown",
				"punctuation.definition.string.begin.markdown",
				"punctuation.definition.string.end.markdown",
				"punctuation.definition.metadata.markdown"
			],
			"settings": {
				"foreground": "#757575"
			}
		},
		{
			"scope": [
				"meta.link.inline.markdown",
				"meta.link.reference.markdown",
				"meta.link.reference.def.markdown",
				"string.other.link.title.markdown",
				"string.other.link.description.markdown",
				"string.other.link.description.title.markdown"
			],
			"settings": {
				"foreground": "#FFA53E"
			}
		},
		{
			"scope": [
				"markup.underline.link.markdown",
				"markup.underline.link.image.markdown"
			],
			"settings": {
				"foreground": "#FFA53E7F",
				"fontStyle": "underline"
			}
		}, 
		{
			"scope": [
				"constant.other.reference.link.markdown"
			],
			"settings": {
				"foreground": "#FFA53E7F"
			}
		}, 
		
		{
			"scope": [
				"markup.inline.raw.string.markdown"
			],
			"settings": {
				"foreground": "#00C857BF"
			}
		},
		{
			"scope": [
				//HR
				"meta.separator.markdown"
			],
			"settings": {
				"foreground": "#C0EAFE"
			}
		},


		//HTML
		{
			"scope": [
				"entity.name.tag.structure.any.html", //HTMl / body
				"meta.tag.sgml.html"
			],
			"settings": {
				"foreground": "#1491CB"
			}
		},
		{
			"scope": [
				"meta.tag.any.html",
				"meta.tag.block.any.html"
			],
			"settings": {
				"foreground": "#1491CB"
			}
		},
		{
			"scope": [
				"meta.tag.inline.any.html"
			],
			"settings": {
				"foreground": "#65CDFE"
			}
		},
		{
			"scope": [
				"meta.tag.metadata.script.html"
			],
			"settings": {
				"foreground": "#0BDBE7"
			}
		},
		{
			"scope": ["entity.other.attribute-name.html"],
			"settings": {
				"foreground": "#FFA53EBF",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["meta.toc-list.id.html"], //#ids
			"settings": {
				"foreground": "#e2dc2e"
			}
		},
		{
			"scope": ["string.quoted.double.html"],
			"settings": {
				"foreground": "#b3ae23"
			}
		}
		
	]
}
